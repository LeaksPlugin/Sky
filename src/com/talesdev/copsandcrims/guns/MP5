package com.talesdev.copsandcrims.guns;

import com.talesdev.copsandcrims.weapon.Weapon;
import com.talesdev.copsandcrims.weapon.WeaponType;
import org.bukkit.Material;
import org.bukkit.inventory.ItemStack;

/**
 * MP5
 *
 * @author MoKunz and AstaLotte
 */
public class MP5 extends Weapon {

    public MP5() {
        super("MP5", "MP5", blankAliases(), WeaponType.PISTOL);
    }

    /**
     * Create ItemStack from weapon
     *
     * @return A weapon item
     */
    @Override
    public ItemStack createItemStack() {
        return new ItemStack(Material.AIR);
    }

    @Override
    public boolean isWeapon(ItemStack itemStack) {
        return false;
    }
}
